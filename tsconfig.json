{
  "compilerOptions": {
    "target": "ES6",
    "module": "es6",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "out/"
  },
  "include": [
    "src/**/*"
  ]
}

